<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="logImpl" value="LOG4J"/>
		<!-- mapper에서 null값이 왔을때 칼럼의 해당 타입임을 설정한다. 
		   이 설정이 없으면 #{email,jdbcType=VARCHAR}), #{salary, jdbcType=INTEGER}		   
		 -->
		<!-- mapper 에서 null 값이 왔을 때 칼럼의 해당 타입임을 설정한다.
		이 설정이 없으면, #{email,jdbcType=VARCHAR}),#{salary,jdbcType=INTEGER})
		 -->
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>
	<typeAliases>
		<typeAlias type="com.salre.main.login.UserDTO" alias="userVO" />
		<typeAlias type="com.salre.main.myPage.PostDTO" alias="PostDTO" />
		<typeAlias type="com.salre.main.myPage.ReviewDTO" alias="ReviewDTO" />
		<typeAlias type="com.salre.main.myPage.ReportDTO" alias="ReportDTO" />
		<typeAlias type="com.salre.main.myPage.LikeDTO" alias="likeVO" />		
		<typeAlias type="com.salre.main.product.ProductDTO" alias="productVO" />
		<typeAlias type="com.salre.main.product.RegionDTO" alias="regionVO"/>
		<typeAlias type="com.salre.main.loan.LoanDTO" alias="loanVO" />
		<typeAlias type="com.salre.main.notify.NotifyDTO" alias="notifyVO" />
		<typeAlias type="com.salre.main.board.BoardDTO" alias="boardDTO" />
		<typeAlias type="com.salre.main.board.CommentDTO" alias="commentDTO" />
		<typeAlias type="com.salre.main.board.PageDTO" alias="pageDTO" />
		<typeAlias type="com.salre.main.contract.ContractDTO" alias="contractVO" />
		<typeAlias type="com.salre.main.contract.ContractUserDTO" alias="contractuserVO" />
		<typeAlias type="com.salre.main.product.ProductContractDTO" alias="productcontractVO" />
		<typeAlias type="com.salre.main.chat.ChatDTO" alias="chatDTO" />
		<typeAlias type="com.salre.main.chat.ChatRoomDTO" alias="chatRoomDTO" />
	</typeAliases>

</configuration> 
 
